@model IEnumerable<ProjectForHealing.Models.Education>

@{
    ViewData["Title"] = "Index";
}

<h1>Countries</h1>

<ul>
    @if (Model.Count() != 0)
    {
        @foreach (var item in Model)
        {
    <li><h1>@Html.ActionLink(item.Country, "EducationView", new { id = item.ID })</h1></li>

        }

    }
    else
    {

    }


</ul>

   
