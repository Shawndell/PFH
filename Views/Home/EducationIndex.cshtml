@model IEnumerable<ProjectForHealing.Models.Education>

@{
    ViewData["Title"] = "Index";
}
    <div class=" container m-0 m-auto">

        <h1>Learn More about other Cultures</h1>

        <ul >
            @if (Model.Count() != 0)
            {
                @foreach (var item in Model)
                {
                    <li class="list-unstyled text-decoration-none"><h1>@Html.ActionLink(item.Country, "EducationView", new { id = item.ID },new { @class="text-dark" })</h1></li>

                }

            }
            else
            {

            }


        </ul>



    </div>


   
